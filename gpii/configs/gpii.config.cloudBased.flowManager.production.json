{
    "type": "gpii.config.cloudBased.flowManager.production",
    "options": {
        "mainServerPort": 8082,
        "authDBServerPort": 5984,
        "distributeOptions": {
            "preferencesDataSource.production.setUrl": {
                "record": "http://preferences.gpii.net/preferences/%userToken",
                 "target": "{that flowManager preferencesDataSource}.options.url",
                 "priority": "after:preferencesDataSource.setUrl"
            },
            "flowManager.production.matchMakers": {
                "record": "http://localhost:8082",
                "target": "{that flowManager}.options.matchMakers.flat.url",
                "priority": "after:flowManager.development.matchMakers"
            }
        }
    },
    "mergeConfigs": [
        "%flowManager/configs/gpii.flowManager.cloudBased.config.production.json",
        "%flatMatchMaker/configs/gpii.flatMatchMaker.config.development.json"
    ]
}
