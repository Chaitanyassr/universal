{
    "type": "gpii.preferencesServer.config.base",
    "options": {
        "gradeNames": ["fluid.component"],
        "components": {
            "server": {
                "options": {
                    "components": {
                        "flowManager": {
                            "options": {
                                "components": {
                                    "preferencesServer": {
                                        "type": "gpii.preferencesServer"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "distributeOptions": {
            "preferencesServer.dataStore": {
                "record": {
                    "baseUrl": "http://localhost",
                    "port": "{kettle.config}.options.dbServerPort",
                    "dbName": "gpii"
                },
                "target": "{that preferencesServer dataStore}.options.dataSourceConfig"
            }
        }
    },
    "require": "preferencesServer"
}
