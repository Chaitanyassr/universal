{
    "type": "gpii.preferencesServer.config.development",
    "options": {
        "distributeOptions": [
            {
                "record": {
                    "baseUrl": "http://localhost",
                    "port": "{kettle.config}.options.dbServerPort",
                    "dbName": "gpii"
                },
                "target": "{that preferencesServer dataStore}.options.dataSourceConfig"
            }
        ]
    },
    "mergeConfigs": [
        "./gpii.preferencesServer.config.base.json",
        "%pouchManager/configs/gpii.pouchManager.config.base.json"
    ]
}
