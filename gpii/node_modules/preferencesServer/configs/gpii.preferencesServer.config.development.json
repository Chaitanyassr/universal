{
    "type": "gpii.preferencesServer.config.development",
    "options": {
        "gradeNames": "fluid.component",
        "distributeOptions": {
            "rawPreferencesUrl": {
                "record": "http://localhost:%port/rawPreferences/%userToken",
                "target": "{that preferencesServer}.options.urls.rawPreferences"
             },
             "logging": {
                 "record": "true",
                 "target": "{that kettle.server}.options.logging"
             },
             "preferencesServerGrades": {
                 "record": "gpii.preferencesServer.dev",
                 "target": "{that preferencesServer}.options.gradeNames"
             }
        }
    },
    "includes": [
        "./gpii.preferencesServer.config.base.json"
    ]
}