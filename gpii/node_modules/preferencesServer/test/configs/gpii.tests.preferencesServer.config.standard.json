{
    "type": "gpii.tests.preferencesServer.config.standard",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": [{
            "source": "{that}.options.devUrls",
            "target": "{that preferencesServer}.options.urls"
        }],
        "devUrls": {
            "rawPreferences": "file://%preferencesServer/test/data/%userToken.json"
        },
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "logging": true,
                    "port": 8081,
                    "components": {
                        "preferencesServer": {
                            "type": "gpii.preferencesServer",
                            "options": {
                                "components": {
                                    "rawPreferencesDataSource": {
                                        "type": "gpii.preferencesServer.tests.testServerMock"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "includes": [
        "%preferencesServer/configs/gpii.preferencesServer.config.base.json"
    ]
}