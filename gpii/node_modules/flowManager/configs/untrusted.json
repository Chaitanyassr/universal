{
    "typeName": "flowManager.untrusted",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.flowManagerUntrustedGrades",
            "target": "{that flowManager}.options.gradeNames"
        }, {
            "source": "{that}.options.devUrls",
            "target": "{that flowManager}.options.urls"
        }],
        "flowManagerUntrustedGrades": ["kettle.urlExpander.distributeDevVariables", "gpii.flowManager.untrusted"],
        "devUrls": {
            "deviceReporter": "http://localhost:%port/device",
            "settings": "http://localhost:8088/%userToken/untrusted-settings/%device"
        }
    },
    "includes": [
        "./base.json",
        "./io.json"
    ]
}
