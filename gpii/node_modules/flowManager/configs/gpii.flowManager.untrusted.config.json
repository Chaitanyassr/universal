{
    "type": "gpii.flowManager.untrusted.config",
    "options": {
        "gradeNames": "fluid.component",
        "distributeOptions": [{
            "source": "{that}.options.untrustedFlowManagerGrades",
            "target": "{that flowManager}.options.gradeNames"
        }, {
            "source": "{that}.options.untrustedFlowManagerUrls",
            "target": "{that flowManager}.options.urls"
        }],
        "untrustedFlowManagerGrades": ["gpii.flowManager.untrusted"],
        "untrustedFlowManagerUrls": {
            "settings": "http://localhost:8088/%userToken/untrusted-settings/%device"
        }
    },
    "includes": [
        "./gpii.flowManager.config.base.json"
    ]
}
