    "typeName": "gpii.flowManager.config.ws",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": {
            "flowManagerGrades.ws": {
                "source": ["gpii.flowManager.save", "gpii.flowManager.ws"],
                "target": "{that flowManager}.options.gradeNames"
             },
             "serverGrades.ws": {
                 "record": "kettle.server.ws",
                 "target": "{that server}.options.gradeNames"
             }
        }
    }
}
