{
    "type": "gpii.flowManager.tests.update.config",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": [{
            "source": "{that}.options.solutionsRegistryUrl",
            "target": "{that flowManager}.options.urls.solutionsRegistry"
        }, {
            "source": "{that}.options.rawPreferencesSourceUrl",
            "target": "{that rawPreferencesServer}.options.rawPreferencesSourceUrl"
        }, {
            "source": "{that}.options.installedSolutionsUrl",
            "target": "{that deviceReporter}.options.installedSolutionsUrl"
        }],
        "rawPreferencesSourceUrl": "file://%flowManager/test/data/%userToken.json",
        "solutionsRegistryUrl": "file://%flowManager/test/data/update_solutionsRegistry.json",
        "installedSolutionsUrl": "file://%flowManager/test/data/update_deviceReporter.json"
    },
    "includes": "./gpii.flowManager.tests.config.base.json"
}
