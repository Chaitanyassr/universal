{
    "type": "gpii.tests.acceptance.untrusted.production",
    "options": {
        "distributeOptions": {
            "flowManager.defaultFlowManagerBaseUrl": {
                "record": "http://flowmanager:9082",
                "target": "{that flowManager untrustedSettingsDataSource}.options.flowManagerBaseUrl"
            },
            "flowManager.production.flowManagerBaseUrl": {
                "record": "@expand:kettle.resolvers.env(FLOWMANAGER_URL)",
                "target": "{that flowManager untrustedSettingsDataSource}.options.flowManagerBaseUrl",
                "priority": "after:flowManager.defaultFlowManagerBaseUrl"
            }
        }
    },
    "mergeConfigs": [
        "%gpii-universal/gpii/configs/gpii.config.untrusted.development.json5"
    ]
}
